<div class="row">
  <div class="col-md-12">
    <h3 class="page-header"><center> 
      <font size="6"><%=@course.name%> at <%=@course.school.name%></font>
    </center></h3>
  </div>
</div>
        
<div class="row">
  <div class="col-md-4"></div>

  <div class="col-md-4">
	<div class="panel panel-info">
	  <div class="panel-heading"><h3> Available Tutors: </h3></div>
	  <div class="panel-body">
		<% @course.tutors.each do |tutor| %>
		<p>
		  <% if tutor.available %>
		  <font size="3"><%= link_to tutor.user.try(:name), tutor_url(tutor) %></font>
		  <% end %>
		</p>
		<%end%>
	  </div>
	</div>
  </div>

  <div class="col-md-4"></div>
</div>