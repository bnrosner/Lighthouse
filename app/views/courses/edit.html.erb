<%= form_for @course do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
    <%= form.label :course_number %>
    <%= form.text_field :course_number, class: "form-control" %>
  </div>
  <%= form.submit class: "btn btn-success" %>
<% end %>