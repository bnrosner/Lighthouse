<%= form_for @tutor do |form| %>
  <div class="form-group">
    <%= form.label :user_id %>
    <%= form.collection_select(:user_id, User.all, :id, :name, {}, {class: "form-control"})%>
    <%= form.label :course_id %>
    <%= form.collection_select(:course_id, Course.all, :id, :name, {}, {class: "form-control"})%>
  </div>
  <%= form.button 'Become a Tutor', class: "btn btn-success" %>
<% end %>